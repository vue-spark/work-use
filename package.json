{
  "name": "@vue-spark/compose-kit",
  "type": "module",
  "version": "1.0.0-beta.1",
  "packageManager": "pnpm@10.11.0",
  "description": "面向复杂业务的组合式 Vue 工具包：可复用的 composables、SFC 组件与通用工具，显著提升开发效率。",
  "author": "leihaohao <https://github.com/l246804>",
  "license": "MIT",
  "homepage": "https://github.com/vue-spark/compose-kit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-spark/compose-kit.git"
  },
  "bugs": {
    "url": "https://github.com/vue-spark/compose-kit/issues"
  },
  "keywords": [
    "web",
    "function",
    "use",
    "hook",
    "composable",
    "element-plus"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/shared/index.js",
    "./element-plus": "./dist/element-plus/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/shared/index.js",
  "module": "./dist/shared/index.js",
  "types": "./dist/shared/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "play": "vite",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier . --write --cache --cache-location ./node_modules/.cache/.prettier-cache",
    "lint": "eslint . --cache --cache-location ./node_modules/.cache/.eslint-cache",
    "lint:fix": "pnpm run lint --fix",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "element-plus": "^2.11.5",
    "vue": "^3.5.22"
  },
  "peerDependenciesMeta": {
    "element-plus": {
      "optional": true
    }
  },
  "dependencies": {
    "vue-component-type-helpers": "^3.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@antfu/utils": "^9.3.0",
    "@sxzz/prettier-config": "^2.2.3",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^14.0.0",
    "bumpp": "^10.2.0",
    "element-plus": "^2.11.5",
    "eslint": "^9.30.1",
    "eslint-plugin-format": "^1.0.1",
    "happy-dom": "^20.0.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "simple-git-hooks": "^2.13.0",
    "tsdown": "^0.15.4",
    "tsx": "^4.20.3",
    "type-fest": "^5.1.0",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-raw": "^0.6.3",
    "unplugin-vue": "^7.0.3",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-jsx": "^0.8.1",
    "unplugin-vue-router": "^0.16.0",
    "vite": "npm:rolldown-vite@^7.1.19",
    "vitest": "^3.2.4",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "vue-tsc": "^3.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
